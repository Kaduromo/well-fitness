{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/layout/*": ["./src/layout/*"],
      "@/hooks/*": ["./src/hooks/*"]
    }
  },
  "include": ["next-env-copy.d.ts", "declarations.d.ts", "**/*.ts", "**/*.tsx", "src/dynamic_adapt.js"],
  "exclude": ["node_modules"]
}
